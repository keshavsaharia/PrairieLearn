<nav class="navbar navbar-expand-sm navbar-dark bg-dark navbar-static-top" role="navigation">
  <a class="navbar-brand"  href="<%= plainUrlPrefix %>">PrairieLearn</a>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-collapse">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbar-collapse">
    <ul class="nav navbar-nav mr-auto" id="main-nav">
      <li class="nav-item">
        <a class="nav-link" href="<%= plainUrlPrefix %>">Home</a>
      </li>

      <li class="nav-item">
        <a class="nav-link" href="<%= urlPrefix %>/assessments">
          <%= course.short_name %>, <%= course_instance.short_name %>
        </a>
      </li>

      <li class="nav-item <% if (navPage == 'assessments') { %>active<% } %>">
        <a class="nav-link" href="<%= urlPrefix %>/assessments">Assessments</a>
      </li>

      <% if (typeof assessment_instance_label != 'undefined' &&
             typeof assessment_instance != 'undefined') { %>
      <li class="nav-item <% if (navPage == 'assessment_instance') { %>active<% } %>">
        <a class="nav-link" href="<%= urlPrefix %>/assessment_instance/<%= assessment_instance.id %>">
          <%= assessment_instance_label %>
        </a>
      </li>
      <% } %>
    </ul>

    <form class="form-inline my-0">

      <a class="btn btn-primary-outline
        <%= (navPage == 'effective') ? 'text-light' : 'text-secondary' %>"
         href="<%= urlPrefix %>/effectiveUser">
        <%= authz_data.user.name %>
      </a>

      <span class="text-secondary">
        <i class="fa fa-<%= (authz_data.role == 'Instructor') ? 'users' : 'user' %>"></i>
        <i class="fa fa-<%= (authz_data.mode == 'Public') ? 'globe' : 'lock' %>"></i>
      </span>

      &nbsp;&nbsp;
      <% if (authz_data.has_instructor_view) { %>
        <div class="dropdown">
          <button class="btn btn-success dropdown-toggle" type="button"
            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              Student <span class="caret"></span>
          </button>
          <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
            <a class="dropdown-item"
               href="<%= plainUrlPrefix %>/course_instance/<%= course_instance.id %>/instructor/assessments">
              Instructor view
            </a>
          </div>
        </div>
      <% } %>

      &nbsp;&nbsp;
      <% if (devMode) { %>
        <a class="btn btn-outline-primary"
           href="<%= urlPrefix %>/loadFromDisk">
          Load from disk
        </a>
      <% } %>
      &nbsp;&nbsp;
      <%- include('logout'); %>
    </form>
  </div>


