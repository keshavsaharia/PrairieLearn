<% if (typeof authn_user != 'undefined') { %>
   <% if (authn_user.provider == 'shibboleth') { %>
       <a class="btn btn-outline-secondary" href="<%= plainUrlPrefix %>/logout">Log out</a>
   <% } else if (authn_user.provider == 'azure') { %>
       <a class="btn btn-outline-secondary" href="<%= config.azureDestroySessionUrl %>">Log out</a>
   <% } else { %>
       <button class="btn btn-outline-secondary" disabled>Log out</button>
   <% } %>
<% } else { %>
   <button class="btn btn-outline-secondary" disabled>Log out</button>
<% } %>
