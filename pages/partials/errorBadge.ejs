<% if (count > 0) { %>
    <% if (!(typeof suppressLink != 'undefined' && suppressLink)) { %>
        <% if (typeof errorQid != 'undefined') { %>
            <a href="<%= urlPrefix %>/errors?qid=<%= errorQid %>">
        <% } else { %>
            <a href="<%= urlPrefix %>/errors">
        <% } %>
    <% } %>
    <span class="badge badge-danger"><%= count %></span>
    <% if (!(typeof suppressLink != 'undefined' && suppressLink)) { %>
        </a>
    <% } %>
<% } %>
